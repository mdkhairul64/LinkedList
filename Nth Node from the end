 ***here is my simple brute force
 
 int getNthFromLast(Node *head, int n)
    {
        if(head==NULL)
        {
            return -1;
        }
        
        Node* temp=head;
        
        int count=0;
        while(temp!=NULL)
        {
            count++;
            temp=temp->next;
        }
        
       // Node* newHead=temp;
        
        if(n>count)
        {
            return -1;
        }
        int target=count-n;
        
        while(target--)
        {
            head=head->next;
        }
        return head->data;
        
    }
